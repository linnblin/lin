Part = Instance.new("Part", game.Workspace)
Part.Anchored = true
Part.CanCollide = false
Part.CastShadow = false
Part.Material = "ForceField"
Part.Shape = "Ball"

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/nahida-cn/Roblox/main/UI.lua"))()

local Window = Library:Window("林", "刀刃球")

Player = Window:Tab("玩家")

Player:Slider("移动速度", 0, 500, game.Players.LocalPlayer.Character.Humanoid.WalkSpeed, function(Value)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
end)

Player:Slider("重力", 0, 500, math.round(game.Workspace.Gravity), function(Value)
    game.Workspace.Gravity = Value
end)

Player:Button("飞行", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/nahida-cn/Roblox/main/Fly.lua"))()
end)

Player:Toggle("踏空", false, function(Value)
    Jump = Value
    game.UserInputService.JumpRequest:Connect(function()
        if Jump then
            game.Players.LocalPlayer.Character.Humanoid:ChangeState("Jumping")
        end
    end)
end)

Player:Toggle("穿墙", false, function(Value)
    Noclip = Value
    if not Noclip then
        game.Players.LocalPlayer.Character.Humanoid:ChangeState("Jumping")
    end
end)

Loop = Window:Tab("循环")

Loop:Textbox("用户名", true, function(Value)
    User = Value
end)

Loop:Toggle("传送", false, function(Value)
    LT = Value
    if not LT then
        game.Players.LocalPlayer.Character.Humanoid:SetStateEnabled("Seated", true)
    end
end)

Auto = Window:Tab("自动")

Auto:Toggle("击退", false, function(Value)
    Parry = Value
end)

Auto:Toggle("传送", false, function(Value)
    Teleport = Value
end)

ESP = Window:Tab("透视")

ESP:Toggle("范围", false, function(Value)
    Aura = Value
end)

Other = Window:Tab("其他")

Other:Button("建筑工具", function()
    for i = 3, 4 do
        HB = Instance.new("HopperBin", game.Players.LocalPlayer.Backpack)
        HB.BinType = i
    end
end)

Other:Button("点击传送", function()
    Tool = Instance.new("Tool", game.Players.LocalPlayer.Backpack)
    Tool.RequiresHandle = false
    Tool.Activated:Connect(function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer:GetMouse().Hit+Vector3.new(0, 2.5, 0)
    end)
end)

Other:Toggle("夜视", false, function(Value)
    Light = Value
    if not Light then
        game.Lighting.Ambient = Color3.new(0, 0, 0)
    end
end)

About = Window:Tab("关于")

About:Label("作者：林")

About:Button("复制群聊", function()
    setclipboard("")
end)

game.RunService.Stepped:Connect(function()
    if Noclip then
        for i, v in pairs(game.Players.LocalPlayer.Character.Parent[game.Players.LocalPlayer.Character.Name]:GetChildren()) do
            if v:IsA("BasePart") then
                v.CanCollide = false
            end
        end
    end
    if LT then
        for i, v in pairs(game.Players:GetPlayers()) do
            if string.find(v.Name, User) then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Character.HumanoidRootPart.CFrame
                game.Players.LocalPlayer.Character.Humanoid:SetStateEnabled("Seated", false)
            end
        end
    end
    if #game.Workspace.Balls:GetChildren() == 0 or not Aura then
        Part.Size = Vector3.zero
    end
    Part.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
    for i, v in pairs(game.Workspace.Balls:GetChildren()) do
        if v:GetAttribute("realBall") then
            Velocity = (v.zoomies.VectorVelocity-game.Players.LocalPlayer.Character.Humanoid:GetMoveVelocity()).Magnitude/2
            if Parry and game.Players.LocalPlayer.Character:FindFirstChild("Highlight") then
                if Teleport and Velocity ~= 0 then
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
                end
                if (v.Position-game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude<Velocity then
                    game.ReplicatedStorage.Remotes.ParryButtonPress:Fire()
                end
            end
            if Aura then
                if Velocity>30 then
                    Velocity = 30
                end
                Part.Size = Vector3.new(Velocity, Velocity, Velocity)
                if game.Players.LocalPlayer.Character:FindFirstChild("Highlight") then
                    Part.Color = Color3.new(1, 0, 0)
                else
                    Part.Color = Color3.new(0, 1, 0)
                end
            end
        end
    end
    if Light then
        game.Lighting.Ambient = Color3.new(1, 1, 1)
    end
end)